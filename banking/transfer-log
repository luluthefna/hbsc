$(document).ready(function() { 
    $('#demo2').click(function() { 
        $.blockUI({ 
          message: 'Transaction In Progress',
          css: { 
            border: 'none', 
            padding: '15px', 
            backgroundColor: '#000', 
            '-webkit-border-radius': '10px', 
            '-moz-border-radius': '10px', 
            opacity: '.5', 
            color: '#fff',
          	fontSize: '18px',
            fontFamily: 'Verdana,Arial',
          	fontWeight: 200,
        } }); 
 
        setTimeout($.unblockUI, 100000); 
    }); 
});

